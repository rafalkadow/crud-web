@using Domain.Modules.Base.ViewModels
@using Shared.Enums
@using Shared.Web
@using Domain.Modules.Base.Consts
@using Domain.Modules.Base.Enums
@model ValueViewModel

<script type="text/javascript">

    var DataPageAction = function () {

        var FormDeleteActionFunction = function () {
            var action = "@MyHttpContext.AppBaseUrl@Model.ControllerNameUrl()@Model.GetAction(@OperationEnum.Delete)";
            return action;
        };

        return {
            FormDeleteAction: function () {
                return FormDeleteActionFunction();
            },
        };
    }();
</script>
